{
  "mcpServers": {
    "hgnc-docker": {
      "command": "docker",
      "args": [
        "run",
        "--rm",
        "-i",
        "-v",
        "hgnc-cache:/home/hgnc/.cache/hgnc",
        "ghcr.io/armish/hgnc.mcp:latest",
        "/usr/local/bin/hgnc-mcp-server",
        "--stdio"
      ],
      "env": {}
    }
  }
}
